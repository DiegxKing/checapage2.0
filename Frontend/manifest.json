{
  "manifest_version": 3,
  "name": "ChecaPage",
  "version": "1.0",
  "description": "jeje.",
  "permissions": [
    "tabs",
    "webRequest",
    "storage",
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/icon_checa_page_completo.png",
      "48": "img/icon_checa_page_completo.png",
      "128": "img/icon_checa_page_completo.png"
    }
  },
  "icons": {
    "16": "img/icon_checa_page_completo.png",
    "48": "img/icon_checa_page_completo.png",
    "128": "img/icon_checa_page_completo.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["overlay.js", "img/*.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
